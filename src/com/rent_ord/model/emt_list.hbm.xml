<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rent_ord.model.EmtListVO" table="EMT_LIST" >
        <composite-id>
	        <key-many-to-one name="roVO" class="com.rent_ord.model.RentOrdVO" lazy="false">
	        	<column name="rentno" />
	        </key-many-to-one>
	        <key-many-to-one name="emtVO" class="com.rent_ord.model.EquipmentVO" lazy="false">
	        	<column name="emtno" />
	        </key-many-to-one>          	
        </composite-id>
    </class>
</hibernate-mapping>